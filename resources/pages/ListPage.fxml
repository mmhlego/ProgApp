<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="root" alignment="TOP_CENTER" prefHeight="600.0" prefWidth="775.0" styleClass="root" stylesheets="@../styles/ListPage.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ListPage">
   <children>
      <Label fx:id="TitleLBL" alignment="CENTER" minWidth="725.0" prefHeight="40.0" text="Title">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Label>
      <HBox fx:id="TitleBar" alignment="CENTER_RIGHT" prefHeight="40.0" spacing="8.0">
         <children>
            <Button fx:id="HomeBTN" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="13.0" prefWidth="14.0" style="-fx-background-radius: 10; -fx-background-color: #3098d9;">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="MinimizeBTN" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="13.0" prefWidth="14.0" style="-fx-background-radius: 10; -fx-background-color: #3dd92f;">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="MaximizeBTN" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="13.0" prefWidth="14.0" style="-fx-background-radius: 10; -fx-background-color: #ff9900;">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="CloseBTN" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="13.0" prefWidth="14.0" style="-fx-background-radius: 10; -fx-background-color: #FF0000;">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
         <padding>
            <Insets right="12.0" />
         </padding>
         <VBox.margin>
            <Insets top="-40.0" />
         </VBox.margin>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </HBox>
      <HBox fx:id="HeaderBox" alignment="CENTER" prefHeight="80.0" />
      <ScrollPane fx:id="ScrollPane" hbarPolicy="NEVER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="410.0" styleClass="ScrollPane">
         <content>
            <VBox fx:id="ScrollBox" alignment="TOP_CENTER" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="725.0" spacing="25.0">
               <padding>
                  <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
